echo \#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#
echo \# $1 
echo ----------------------------------
echo \# Header line:     
documentcat $1 | head --lines=1 
echo ----------------------------------
echo \# Matches:               
documentcat $1 | ack $2
echo \#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#\#

 